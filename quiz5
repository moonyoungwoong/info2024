import pandas as pd
 
dataset = [
    {"챔피언": "아리", "라인": "미드", "승리": 1},
    {"챔피언": "가렌", "라인": "탑", "승리": 1},
    {"챔피언": "아리", "라인": "미드", "승리": 1},
    {"챔피언": "가렌", "라인": "미드", "승리": 0},
    {"챔피언": "아리", "라인": "바텀", "승리": 0},
    {"챔피언": "가렌", "라인": "탑", "승리": 0},
    {"챔피언": "아리", "라인": "바텀", "승리": 1}
]
 
df = pd.DataFrame(dataset)
ndf = df.groupby("챔피언").size()
ndf = df.groupby(["챔피언", "라인"]).mean()
ndf = df.groupby("챔피언").sum()
 
print(ndf)
